{
  "name": "jest",
  "version": "1.0.0",
  "description": "Jest Test",
  "main": "sum.js",
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-jest": "^12.0.2",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.12.5",
    "browserify": "^13.0.0",
    "eslint": "^2.9.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "imagemin-cli": "^2.1.0",
    "jest-cli": "^11.0.2",
    "node-sass": "^3.6.0",
    "onchange": "^2.4.0",
    "parallelshell": "^2.0.0",
    "postcss-cli": "^2.5.2",
    "susy": "^2.2.12",
    "vinyl-source-stream": "^1.1.0"
  },
  "scripts": {
    "test": "jest",
    "scss": "node-sass --output-style expanded --indent-type tab --indent-width 1 -o assets/css build/_sass",
    "autoprefixer": "postcss -u autoprefixer -r assets/css/*",
    "lint": "eslint build/js",
    "imagemin": "imagemin build/images assets/images -p",
    "serve": "browser-sync start --proxy 'jest.dev' --files 'assets/css/*.css, assets/js/*.js'",
    "build:css": "npm run scss && npm run autoprefixer",
    "browserify": "browserify build/js/main.js -o assets/js/bundle.js -t [ babelify --presets [ es2015 ] ]",
    "watch:css": "onchange 'build/_sass/*.scss' -- npm run build:css",
    "watch:js": "onchange 'build/js/*.js' -- npm run browserify",
    "watch:all": "parallelshell 'npm run serve' 'npm run watch:css' 'npm run watch:js'"
  },
  "author": "Dan Blake",
  "license": "ISC"
}
